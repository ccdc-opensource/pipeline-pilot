						<sci:metaproplist>
							<sci:propval name="SCI_InitializeUsingRevertInfo" base64="UI1">Al1HAAFmVGhlIG5hbWUgb2YgdGhlIGpvdXJuYWwgdG8gY2hlY2ssIG9yIHRoZSBuYW1lIG9mIGEgcHJvcGVydHkgd2hpY2ggY29udGFpbnMgdGhlIGpvdXJuYWwgbmFtZSB0byBjaGVjay4sU3RyaW5nVHlwZQAMAAAAAQEAAAEcVGhpcyBwYXJhbWV0ZXIgaXMgcmVxdWlyZWQuBAD8AAAAAAAAAAAANEpvdXJuYWwgTmFtZQPMU8+7BAA=</sci:propval>
							<sci:propval name="SCI_InitializeUsingOrigType">StringType</sci:propval>
