						<sci:help>The PilotScript expression used to filter the data.</sci:help>
						<sci:member>Initial Expression</sci:member>
						<sci:member>Final Expression</sci:member>
						<sci:member>Keep Calculated Properties</sci:member>
						<sci:value>#from_file := Parameter(&apos;Query Source&apos;) Ne &apos;From Tag&apos;;

// How is query file specified? From file?
If #from_file Then
	// From file: for the report, this needs to be moved to a more convenient location.
	#source := Parameter(&apos;Source&apos;);
	#path := RSplit(#source, &apos;\\|/&apos;, &apos;g&apos;);
	destination := @tempDir . &apos;/&apos; . #path[NumValues(#path)];
	@_query_file := destination;
Else
	// From tag: derive format for the file to create.
	If Parameter(&apos;Format&apos;) Eq &apos;SDF&apos; Then
		@_query_file := @tempDir . &apos;/query_file.sdf&apos;;
	Else
		@_query_file := @tempDir . &apos;/query_file.mol2&apos;;
	End If;
End If;

// Create screening file path.
If Parameter(&apos;Format&apos;) Eq &apos;SDF&apos; Then
	@_screening_file := @tempDir. &apos;/screening_file.sdf&apos;;
Else
	@_screening_file := @tempDir . &apos;/screening_file.mol2&apos;;
End If;

#from_file;</sci:value>
