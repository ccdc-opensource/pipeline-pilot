	if #nl &lt;= 0 then
		#nl := FindSubstring(#displayName, &quot;\n&quot;);
	end if;

	/* Keep the first line only */
	if (#nl &gt; 0) then
		StrErase(#displayName, #nl);
	end if;
	
	/* If the first line starts with a comment - remove comment marking. */
	If #displayName Like &apos;/*%&apos; Then
