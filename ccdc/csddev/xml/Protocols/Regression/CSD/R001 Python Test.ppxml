								<sci:member>Keep Calculated Properties</sci:member>
								<sci:value>#switches := RGlobalPropertyNames(&apos;Arguments Argument \d+ Switch&apos;);
#values := RGlobalPropertyNames(&apos;Arguments Argument \d+ Value&apos;);

@_args := Array();

For #i In 1 .. NumValues(#switches) Loop
	If #switches[#i] Ne &apos;&apos; Then
		Append(@_args, GlobalProperty(#switches[#i]));
	End If;
	If #values[#i] Ne &apos;&apos; Then
		Append(@_args, GlobalProperty(#values[#i]));
	End If;
End Loop;

Contract(&apos; &apos;, @_args);</sci:value>
