			<sci:data object="SciTegic.EvaluateExpression.1">
				<sci:component name="Posttest Global Leakage" version="2">
					<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}" uimpl="true">
						<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate</sci:help>
						<sci:member>Initial Expression</sci:member>
						<sci:member>Final Expression</sci:member>
						<sci:value>/* If @&apos;_LG_Hash&apos; has not been defined, there has not been a pretest leakage component yet */
if (@&apos;_LG_Hash&apos; is not defined) then
	error(&quot;PreTest Global Leakage must appear before Posttest Global Leakage&quot;);
end if;

/* the number of globals stored by pretest global leakage is #NG, the number of current globals is #NG2 */
#NG2 := numglobalproperties();
resize(Leaks, 0);

for #i in 1 .. #NG2
loop
	#glob2 := ithglobalpropertyname(#i);
	/* Search the stored global names and if we do not find a match, then add the global name to the leaks property array */
	if (NumValues(HashFind(@&apos;_LG_Hash&apos;, #glob2)) = 0) then
		append(Leaks, #glob2);
	end if;
end loop;

/* Change the Leaks array into a comma separated list */
contract(&apos;,&apos;, Leaks);
@Leaks := Leaks;

/* clean up the global list */
HashDestroy(@&apos;_LG_Hash&apos;);
</sci:value>
					</sci:arg>
