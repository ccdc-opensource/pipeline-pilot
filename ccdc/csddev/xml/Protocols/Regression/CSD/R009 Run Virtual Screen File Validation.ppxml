			<sci:data object="SciTegic.Protocol.1">
				<sci:component name="Run Virtual Screening" version="2">
					<sci:arg name="Screening Set" type="URLType" required="true" visibility="visible" multi="false" legalvalsenforced="false">
						<sci:metaproplist>
