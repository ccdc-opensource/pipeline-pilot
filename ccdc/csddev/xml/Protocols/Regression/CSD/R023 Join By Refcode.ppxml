			<sci:data object="SciTegic.EvaluateExpression.1">
				<sci:component name="Tag Regression Record" version="2">
					<sci:arg name="Success" type="BoolType" required="true" visibility="visible" multi="false" legalvalsenforced="false">
						<sci:help>Tag the record as a success or otherwise.</sci:help>
