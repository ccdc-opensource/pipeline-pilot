						<sci:legalval>Calculate Density</sci:legalval>
						<sci:legalval selected="true">CCDC Number</sci:legalval>
						<sci:legalval selected="true">Chemical Name</sci:legalval>
						<sci:legalval>Color</sci:legalval>
