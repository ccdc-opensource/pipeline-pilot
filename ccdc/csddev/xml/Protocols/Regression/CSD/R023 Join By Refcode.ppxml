						<sci:legalval selected="true">Is Organometallic</sci:legalval>
						<sci:legalval selected="true">Is Polymeric</sci:legalval>
						<sci:legalval selected="true">Is Powder Study</sci:legalval>
						<sci:legalval>Melting Point</sci:legalval>
						<sci:legalval>Peptide Sequence</sci:legalval>
