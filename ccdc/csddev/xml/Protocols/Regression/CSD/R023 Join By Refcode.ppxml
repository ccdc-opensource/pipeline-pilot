						<sci:legalvalscript>#names := RunProtocol(&apos;Components/CSD/Utilities/Gather Database Names&apos;);
If Not Contains(#names, &apos;&apos;) Then
	InsertValue(#names, &apos;&apos;, 1);
End If;
SetLegalValues(#names);</sci:legalvalscript>
